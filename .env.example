# SonGo Enterprise Shipping Platform - Environment Configuration
# Copy this file to .env and fill in your actual values

# =============================================================================
# AWS CONFIGURATION
# =============================================================================
AWS_REGION=us-east-1
AWS_ACCESS_KEY_ID=your_aws_access_key_id
AWS_SECRET_ACCESS_KEY=your_aws_secret_access_key
AWS_ACCOUNT_ID=123456789012

# DynamoDB Configuration
DYNAMODB_ENDPOINT=http://localhost:4566
USERS_TABLE=songo-enterprise-users
QUOTES_TABLE=songo-enterprise-quotes
SHIPMENTS_TABLE=songo-enterprise-shipments
TRACKING_EVENTS_TABLE=songo-enterprise-tracking-events
PAYMENTS_TABLE=songo-enterprise-payments

# S3 Configuration
S3_DOCUMENTS_BUCKET=songo-enterprise-documents
S3_LABELS_BUCKET=songo-enterprise-labels
S3_ANALYTICS_BUCKET=songo-enterprise-analytics

# SNS/SQS Configuration
QUOTE_NOTIFICATIONS_TOPIC=arn:aws:sns:us-east-1:123456789012:songo-enterprise-quote-notifications
PAYMENT_NOTIFICATIONS_TOPIC=arn:aws:sns:us-east-1:123456789012:songo-enterprise-payment-notifications
TRACKING_NOTIFICATIONS_TOPIC=arn:aws:sns:us-east-1:123456789012:songo-enterprise-tracking-notifications
SYSTEM_ALERTS_TOPIC=arn:aws:sns:us-east-1:123456789012:songo-enterprise-system-alerts

QUOTE_PROCESSING_QUEUE=https://sqs.us-east-1.amazonaws.com/123456789012/songo-enterprise-quote-processing
TRACKING_UPDATES_QUEUE=https://sqs.us-east-1.amazonaws.com/123456789012/songo-enterprise-tracking-updates

