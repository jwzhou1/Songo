CANADA_POST_SECRET_KEY=your_canada_post_secret_key
CANADA_POST_CUSTOMER_NUMBER=123456789

# =============================================================================
# DATABASE CONFIGURATION
# =============================================================================
# PostgreSQL (for relational data)
POSTGRES_HOST=localhost
POSTGRES_PORT=5432
POSTGRES_USER=songo_user
POSTGRES_PASSWORD=secure_password_123
POSTGRES_DB=songo_enterprise

# Redis (for caching and sessions)
REDIS_HOST=localhost
REDIS_PORT=6379
REDIS_PASSWORD=redis_secure_password_123
REDIS_URL=redis://:redis_secure_password_123@localhost:6379

# =============================================================================
# APPLICATION CONFIGURATION
# =============================================================================
NODE_ENV=development
PORT=3000
FRONTEND_URL=http://localhost:4200
API_URL=http://localhost:3000

# JWT Configuration
JWT_SECRET=your_super_secret_jwt_key_min_32_chars
JWT_EXPIRES_IN=24h
JWT_REFRESH_EXPIRES_IN=7d

# Session Configuration
SESSION_SECRET=your_session_secret_key_min_32_chars
SESSION_TIMEOUT=3600000

# =============================================================================
# MONITORING & LOGGING
# =============================================================================
# Elasticsearch
ELASTICSEARCH_URL=http://localhost:9200
ELASTICSEARCH_INDEX=songo-enterprise-logs

# Grafana
GRAFANA_PASSWORD=admin_password_123
GRAFANA_URL=http://localhost:3001

# Prometheus
PROMETHEUS_URL=http://localhost:9090

# Jaeger Tracing
JAEGER_ENDPOINT=http://localhost:14268/api/traces

# =============================================================================
# CI/CD CONFIGURATION
# =============================================================================
# Jenkins
JENKINS_URL=http://localhost:8080
JENKINS_USER=admin
JENKINS_TOKEN=your_jenkins_api_token

# Docker Registry
DOCKER_REGISTRY=123456789012.dkr.ecr.us-east-1.amazonaws.com
DOCKER_USERNAME=AWS
DOCKER_PASSWORD=your_ecr_token

# =============================================================================
# NOTIFICATION SERVICES
# =============================================================================
# Microsoft Teams Webhook
TEAMS_WEBHOOK_URL=https://outlook.office.com/webhook/your-webhook-url

# Slack Configuration
SLACK_BOT_TOKEN=xoxb-your-slack-bot-token
SLACK_CHANNEL=#songo-deployments

# Email Configuration (SES)
SES_REGION=us-east-1
SES_FROM_EMAIL=noreply@songo-enterprise.com
SES_REPLY_TO_EMAIL=support@songo-enterprise.com

# =============================================================================
# SECURITY CONFIGURATION
# =============================================================================
# Rate Limiting
RATE_LIMIT_WINDOW_MS=900000
RATE_LIMIT_MAX_REQUESTS=100

# CORS Configuration
CORS_ORIGIN=http://localhost:4200,https://songo-enterprise.vercel.app
CORS_CREDENTIALS=true

# Encryption
ENCRYPTION_KEY=your_32_character_encryption_key_here
ENCRYPTION_ALGORITHM=aes-256-gcm

# =============================================================================
# FEATURE FLAGS
# =============================================================================
ENABLE_REAL_TIME_TRACKING=true
ENABLE_PAYMENT_PROCESSING=true
ENABLE_MULTI_CARRIER_QUOTES=true
ENABLE_GPS_TRACKING=true
ENABLE_ANALYTICS=true
ENABLE_NOTIFICATIONS=true
ENABLE_CACHING=true

# =============================================================================
# DEVELOPMENT TOOLS
# =============================================================================
# LocalStack (for local AWS services)
LOCALSTACK_ENDPOINT=http://localhost:4566
LOCALSTACK_HOSTNAME=localhost

# Debug Configuration
DEBUG=songo:*
LOG_LEVEL=debug

# Testing Configuration
TEST_DATABASE_URL=postgresql://test_user:test_password@localhost:5433/songo_test
TEST_REDIS_URL=redis://localhost:6380

# =============================================================================
# PERFORMANCE CONFIGURATION
# =============================================================================
# Connection Pooling
DB_POOL_MIN=2
DB_POOL_MAX=10
DB_POOL_IDLE_TIMEOUT=30000

# Cache Configuration
CACHE_TTL=3600
CACHE_MAX_SIZE=1000

# Request Timeout
REQUEST_TIMEOUT=30000
UPLOAD_TIMEOUT=300000

# =============================================================================
# BUSINESS CONFIGURATION
# =============================================================================
# Default Settings
DEFAULT_CURRENCY=USD
DEFAULT_WEIGHT_UNIT=LB
DEFAULT_DIMENSION_UNIT=IN
DEFAULT_COUNTRY=US

# Pricing Configuration
BASE_SHIPPING_COST=15.00
INSURANCE_RATE=0.01
SIGNATURE_FEE=5.00
PROCESSING_FEE_RATE=0.029
PROCESSING_FEE_FIXED=0.30

# Operational Limits
MAX_PACKAGE_WEIGHT=150
MAX_PACKAGE_DIMENSION=108
MAX_SHIPMENT_VALUE=50000
MAX_PACKAGES_PER_SHIPMENT=10

# =============================================================================
# ENVIRONMENT SPECIFIC OVERRIDES
# =============================================================================
# These will be overridden in production
ENVIRONMENT=development
VERSION=1.0.0
BUILD_NUMBER=local
COMMIT_HASH=local-development

# Health Check Configuration
HEALTH_CHECK_TIMEOUT=5000
HEALTH_CHECK_INTERVAL=30000

# Backup Configuration
BACKUP_ENABLED=false
BACKUP_SCHEDULE=0 2 * * *
BACKUP_RETENTION_DAYS=30
